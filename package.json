{"name":"cypress",
"version":"1.0.0",
"description":"Cypress tests automatizados pero el curso de PIL",
"main":"index.js",
"scripts":{
    "test":"text",
    "cypress:open:desktop":"npx cypress open --e2e --browser chrome -e type=desktop",
    "cypress:open:mobile":"npx cypress open --e2e --browser chrome -e type=mobile",
    "cypress:open:firefox:desktop":"npx cypress open --e2e --browser firefox -e type=desktop,grepTags=@regression",
    "cypress:open:firefox:mobile":"npx cypress open --e2e --browser firefox -e type=mobile",
    "cypress:run:eden":"npx cypress run --e2e --browser chrome --spec cypress/e2e/Eden",
    "cypress:run:eden:firefox":"npx cypress run --e2e --browser firefox --spec cypress/e2e/Eden"},
    "keywords":["cypress","test","automation","PIL"],
    "author":"Nur",
    "license":"ISC",
    "devDependencies":{
        "@bahmutov/cy-grep":"^1.9.17",
        "cypress":"^12.17.4",
        "mochawesome":"^7.1.3"},
        "dependencies":{
            "ajv":"^8.12.0"}}